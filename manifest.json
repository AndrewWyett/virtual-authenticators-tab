{
  "name": "Virtual Authenticator",
  "version": "0.1",
  "description": "An extension that lets you create and manage virtual authenticators",
  "manifest_version": 2,
  "permissions": [
    "debugger", "storage", "tabs"
  ],
  "browser_action": {
    "default_title": "Manage Virtual Authenticators",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "key": ""
}
